       // Use IntelliSense to learn about possible attributes.
       // Hover to view descriptions of existing attributes.
       // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
       "version": "0.2.0",
       "configurations": [
              {
                     "name": "Flutter",
                     "type": "dart",
                     "request": "launch",
                     "program": "lib/main.dart"
              },
             
              {
                     "name": "common-ui-documentation",
                     "request": "launch",
                     "type": "dart"
              },
              {
                     "name": "common-ui-documentation (profile mode)",
                     "request": "launch",
                     "type": "dart",
                     "flutterMode": "profile"
              },
              {
                     "name": "web",
                     "program": "lib/main.dart",
                     "request": "launch",
                     "debugConsole": "integratedTerminal",
                     "type": "dart",
                     "deviceId":"chrome",
                     "args": [
                         "--web-port=5000",
                         "--web-enable-expression-evaluation"
                     ],
                 },
       ]
}